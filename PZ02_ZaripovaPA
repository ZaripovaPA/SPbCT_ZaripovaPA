package com.metanit;
public class Main {
    public static void main(String[] args) {
        System.out.print(1000*60*60*24*7);
        System.out.print(1000*60*60*24*30);
    }
}

7.
package com.metanit;
public class Main {
    public static void main(String[] args) {
        byte x=-128;
        System.out.print(-x);
        byte y=127;
        System.out.print(++y);
    }
}

