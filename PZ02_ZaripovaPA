1. 
package com.company;
public class PZ2 {
    public static void main(String[] args) {
        char symb1=1067;
        char symb2 ='Ð«';
        System.out.println("symb1 contains "+ symb1);
        System.out.println("symb2 contains "+ symb2);
    }
}

2. 
package com.company;
public class PZ2 {
    public static void main(String[] args) {
        char ch = 'J';
        int intCh = (int) ch;
        System.out.println("J corresponds with "+ intCh);
    }
}

3. 
package com.company;
public class PZ2 {
    public static void main(String[] args) {
        String s1 = "I have ";
        String s2 = " apples ";
        int num = 3;
        String s = s1 + num + s2;
        System.out.println(s);
    }
}

4.
package com.company;
public class PZ2 {
    public static void main(String[] args) {
        byte x=5;
        byte y1=x++;
        byte y2=x--;
        byte y3=++x;
        byte y4=--x;
        System.out.println(y1);
        System.out.println(y2);
        System.out.println(y3);
        System.out.println(y4); }
}

