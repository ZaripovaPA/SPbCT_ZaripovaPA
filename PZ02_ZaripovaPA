public class Main {
    public static void main(String[] args) {
        System.out.print(1+2+"text");
        System.out.print("text"+1+2);
    }
}

10.
package com.metanit;
public class Main {
    public static void main(String[] args) {
        char c='A';
        System.out.print(c);
        System.out.print(c+1);
        System.out.print("c="+c);
        System.out.print('c' + '=' + c);
    }
}

11.
package com.metanit;
public class Main {
    public static void main(String[] args) {
        System.out.print(1e20f*1e20f);
        System.out.print(-1e200*1e200);
    }
}

12.
package com.metanit;
public class Main {
    public static void main(String[] args) {
        System.out.print(1e-40f/1e10f);
        System.out.print(-1e-300/1e100);
        float f=1e-6f;
        System.out.print(f);
        f+=0.002f;
        System.out.print(f);
        f+=3;
        System.out.print(f);
        f+=4000;
        System.out.print(f);
    }
}

13.
package com.metanit;
public class Main {
    public static void main(String[] args) {
        double d = 1e-305 * Math.PI;
        System.out.print(d);
        for (int i = 0; i < 4; i++)
            System.out.print(d /= 100000);
    }
}

14.
package com.metanit;
public class Main {
public static void main(String[] args) {
System.out.print(1/2);
System.out.print(1/2.);
}
}

15.
package com.metanit;
public class Main {
public static void main(String[] args) {
int x=3;
int y=5;
System.out.print (x/y);
System.out.print((double)x/y);
System.out.print(1.0*x/y);
}
}

16.
package com.metanit;
public class Main {
public static void main(String[] args) {
long l=111111111111L;
float f = l;
l = (long) f;
System.out.print(l);
}
}
